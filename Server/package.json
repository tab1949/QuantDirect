{
  "name": "marketdirect-server",
  "version": "0.1.0",
  "private": true,
  "main": "dist/index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only -r dotenv/config src/index.ts",
    "build": "tsc -p tsconfig.json",
    "start": "node dist/index.js",
    "lint": "eslint . --ext .ts",
    "test-redis": "node test-redis.js",
    "redis:start": "docker-compose up -d redis",
    "redis:stop": "docker-compose down",
    "redis:logs": "docker-compose logs redis",
    "redis:status": "docker-compose ps redis"
  },
  "dependencies": {
    "@types/axios": "^0.9.36",
    "axios": "^1.12.2",
    "cors": "^2.8.5",
    "dotenv": "^16.1.4",
    "express": "^4.18.2",
    "log4js": "^6.9.1",
    "redis": "^4.6.12"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.17",
    "@types/node": "^20.19.17",
    "@types/swagger-ui-express": "^4.1.3",
    "swagger-ui-express": "^4.6.3",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.2.2"
  }
}
