{
  "name": "quantdirect-server",
  "version": "0.1.0",
  "private": true,
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch --clear-screen=false --include=\"src/**/*\" --exclude=\"node_modules/**\" --exclude=\"dist/**\" src/index.ts",
    "dev:nodemon": "nodemon",
    "dev:chokidar": "chokidar \"src/**/*.ts\" \"src/**/*.js\" \"src/**/*.json\" -c \"tsx src/index.ts\" --initial",
    "build": "tsc -p tsconfig.json",
    "start": "node dist/index.js",
    "lint": "eslint . --ext .ts"
  },
  "dependencies": {
    "@types/axios": "^0.9.36",
    "axios": "^1.12.2",
    "cors": "^2.8.5",
    "dotenv": "^16.1.4",
    "express": "^4.18.2",
    "log4js": "^6.9.1",
    "redis": "^4.6.12"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.17",
    "@types/node": "^20.19.17",
    "@types/swagger-ui-express": "^4.1.3",
    "chokidar-cli": "^3.0.0",
    "nodemon": "^3.0.2",
    "tsx": "^4.20.6",
    "typescript": "^5.2.2"
  }
}
